2052                    CALCULO_NIVEL:
2052 cc 03 ff                   Ldd #1023 ;Maximo numero de 10 bits
2055 ce 00 ff                   Ldx #255   ; Maximo numero de 8 bits
2058 18 10                      Idiv  ;x=4   ;Obtenemos la razon entre estas dos escalas (4)
205a fc 10 10                   Ldd NIVEL_PROM
205d 18 10                      Idiv ;x=NIVEL_PROM/4
205f b7 c5                      Xgdx ;D=NIVEL_PROM/4
2061 86 14                      Ldaa #20  ;Maximo de nivel medido
2063 12                         Mul
2064 ce 00 ff                   Ldx #255
2067 18 10                      Idiv  ;Con eso se obtiene el valor nivel en en X
2069 b7 c5                      Xgdx  ;Se pasa a D
206b c1 0f                      Cmpb #15 ;Maxima altura permitida es 15
206d 23 02                      Bls get_V
206f c6 0f                      Ldab #15
                        
2071                    get_V:
2071 7b 10 12                   Stab NIVEL
2074 86 07                      ldaa #7 ;Area real es 7.07 pero se redondea
2076 12                         Mul ;V= A*H
2077 7b 10 13                   Stab VOLUMEN
                        
                                ;Realizamos las comparaciones de nivel
207a c1 5f                      cmpb #95  ;V>=95
207c 24 1d                      bhs LED_OFF
                        
207e c1 10                      cmpb #16 ;V<=95
2080 23 0b                      bls LED_ON
                        
2082 c1 20                      cmpb #32 ;95>=V>=32
2084 24 26                      bhs NO_PRINT
                                
                                
                                ;32>=V>=16
2086 1f 10 1a 04 1c             brclr BANDERAS,$04,PRINT
208b 20 1f                      bra NO_PRINT
                        
208d                    LED_ON:
208d 18 0b 00 02 68             movb #$00,PTJ        ;habilita led
2092 4c 01 01                   bset PORTB,#$01 ;enciende la bomba
2095 1d 10 1a 04                bclr BANDERAS,#$04 ;MSG_SEL <--- 0 para indicar que el mensaje a imprimir es el de alarma
2099 20 0c                      bra PRINT
                        
209b                    LED_OFF:
209b 18 0b 02 02 68             movb #$02,PTJ ;deshabilida leds
20a0 4d 01 01                   bclr PORTB,#$01 ;se apaga la bomba
20a3 1c 10 1a 04                bset BANDERAS,#$04 ;MSG_SEL <--- 1 para indicar que el mensaje a imprimir es el de tanque lleno
20a7                    PRINT:
20a7 1c 10 1a 02                bset BANDERAS,#$02 ;MSG <-- 1 para indicar que hay que imprimir otro mensaje
20ab 3d                          rts
                        
20ac                    NO_PRINT:
20ac 1d 10 1a 02                bclr BANDERAS,#$02 ;MSG <-- 0 para indicar que NO hay que imprimir otro mensaje
20b0 3d                         rts
                        
