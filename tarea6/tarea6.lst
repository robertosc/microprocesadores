20be 59                                 lsld       ;se realiza el desplazamiento
20bf 75 10 16                           rol BCD_L
20c2 75 10 15                           rol BCD_H
20c5 3b                                 pshd       ;guardamos d en pila para usar a y b
20c6 86 0f                              ldaa #$0F
20c8 b4 10 16                           anda BCD_L
20cb 81 05                              cmpa #5
20cd 25 02                              blo UNIDADES_CONV
20cf 8b 03                              adda #3
