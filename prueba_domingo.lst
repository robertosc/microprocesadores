                        ;------------------------------------------------------------------------------
                        ;   Subrutina BCD_BIN: el arreglo Num_Array corresponde a un numero en BCD donde
                        ;     cada entrada es un digito. Esta subrutina toma este arreglo y calcula en
                        ;     binario el valor numerico del arreglo. El resultado se almacena en ValorVueltas.
                        ;     INPUTS: NUM_ARRAY
                        ;     OUTPUTS:ValorVueltas
                        ;------------------------------------------------------------------------------
238e ce 10 09           BCD_BIN:        ldx #Num_Array
2391 e6 01                              ldab 1,x           ;revisamos si la unidad es distinta de FF
2393 c1 ff                              cmpb #$FF
2395 27 05                              beq UNIDAD         ;si es FF, el valor no es valido
2397 7b 10 02                           stab ValorVueltas       ;Si no, lo guarda en ValorVueltas
239a 20 07                              bra DECENA         ;lee decenas
                        
239c 18 0c 10 09 10 02  UNIDAD:         movb Num_Array,ValorVueltas
23a2 3d                                 rts
                        
23a3                    DECENA:
23a3 87                                 clra
23a4 f6 10 09                           ldab Num_Array    ;carga en b
23a7 cd 00 0a                           ldy #10           ;multiplica decenas por 10
23aa 13                                 emul
                        
23ab fb 10 02                           addb ValorVueltas
23ae 7b 10 02                           stab ValorVueltas
                        
23b1 3d                                 rts
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina BIN_BCD: esta subrutina realiza la conversion de un numero
                        ;     binario entre 0 y 99 (inclusivos) a su representacion en BCD. El numero
                        ;     a convertir se recibe como parametro por el registro A. El resultado en
                        ;     BCD se devuelve por la variable BCD_L, donde el nibble mas significativo son
                        ;     las decenas y el menos significativo las unidades.
                        ;------------------------------------------------------------------------------
23b2                    BIN_BCD:
                        
23b2 c6 07                              Ldab #7  ; Contador B=15
23b4 79 10 1a                           Clr BCD_L
                        
23b7                    lazo:
23b7 48                                 Lsla
23b8 75 10 1a                           Rol BCD_L  ;Lo mismo para la variable BCD_L y BCD_H
23bb 36                                 Psha
                        
23bc b6 10 1a                           Ldaa BCD_L ;Cargamos en A el BCD_L
23bf 84 0f                              Anda #$0F  ;Tomamos solo en cuenta los 4LSB
23c1 81 05                              Cmpa #5   ;Comparamos con 5
23c3 25 02                              Blo men031  ;Si es menor, salte a men031
23c5 8b 03                              Adda #3  ;En caso de mayor, sume 3
                        
23c7                    men031
23c7 7a 10 1d                           Staa LOW  ;Guardamos temporalmente el resultado anterior
                        
23ca b6 10 1a                           Ldaa BCD_L
23cd 84 f0                              Anda #$F0 ;En A tenemos cargado del bit 4 al 7
23cf 81 50                              Cmpa #$50  ;Comparamos con $50
23d1 25 02                              Blo men301
23d3 8b 30                              Adda #$30   ;Si es mayor, sume 30
                        
23d5                    men301
23d5 bb 10 1d                           Adda LOW   ;Se suman los bits para obtener los 4 LSB de resultado
23d8 7a 10 1a                           Staa BCD_L  ;Se guarda el resultado
23db 32                                 Pula
23dc 04 31 d8                           Dbne b, lazo
                        
23df 48                                 Lsla
23e0 75 10 1a                           Rol BCD_L
                        
23e3 3d                                 Rts
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina CONV_BIN_BCD: recibe como parametros de entrada las variables BIN1 y
                        ;     BIN2 y realiza la conversion a BCD de cada una de estas variables. Luego de la conversion, si
                        ;     el numero es menor que 10 significa que el display de 7 segmentos utilizado
                        ;     para las decenas no es necesario que este encendido; en este caso se escribe
                        ;     $B en el nibble mas significativo de BCD1 y BCD2 para indicarlo. Carga $BB
                        ;     en BCD1 o BCD2 dependiendo de si deben estar apagados, mientras que carga
                        ;     $AA si deben tener una raya.
                        ;     INPUTS: BIN1, BIN2
                        ;     OUTPUTS: BCD1, BCD2
                        ;------------------------------------------------------------------------------
23e4                    CONV_BIN_BCD:
23e4 b6 10 16                           Ldaa BIN1
                        
23e7 81 bb                              Cmpa #$BB
23e9 27 06                              Beq BIN1_BB
                        
23eb 81 aa                              Cmpa #$AA
23ed 27 09                              Beq BIN1_AA
                        
23ef 20 0e                              Bra BIN1_CALC
                        
23f1 18 0b bb 10 18     BIN1_BB:        Movb #$BB,BCD1
23f6 20 16                              Bra BIN2_CHECK
                        
23f8 18 0b aa 10 18     BIN1_AA:        Movb #$AA,BCD1
23fd 20 0f                              Bra BIN2_CHECK
                        
                        
23ff                    BIN1_CALC:
23ff 16 23 b2                           Jsr BIN_BCD ;Pasamos BIN1 a BCD
2402 b6 10 1a                           Ldaa BCD_L
2405 81 0a                              Cmpa #10
2407 24 02                              Bhs mayor1
2409 8b b0                              Adda #$B0 ;Si solo tiene un digito, agrega B como "decenas"
                        
240b 7a 10 18           mayor1          Staa BCD1 ;Guardamos el valor en BCD1
                        
                        
240e b6 10 17           BIN2_CHECK:        Ldaa BIN2
                        
2411 81 bb                              Cmpa #$BB
2413 27 06                              Beq BIN2_BB
                        
2415 81 aa                              Cmpa #$AA
2417 27 09                              Beq BIN2_AA
                        
2419 20 0e                              Bra BIN2_CALC
                        
241b 18 0b bb 10 19     BIN2_BB:        Movb #$BB,BCD2
2420 20 16                              Bra FIN_CONV
                        
2422 18 0b aa 10 19     BIN2_AA:        Movb #$AA,BCD2
2427 20 0f                              Bra FIN_CONV
                        
                        
2429                    BIN2_CALC:
2429 16 23 b2                           Jsr BIN_BCD ;Pasamos BIN1 a BCD
242c b6 10 1a                           Ldaa BCD_L
242f 81 0a                              Cmpa #10
2431 24 02                              Bhs mayor2
2433 8b b0                              Adda #$B0 ;Si solo tiene un digito, agrega B como "decenas"
                        
2435 7a 10 19           mayor2          Staa BCD2 ;Guardamos el valor en BCD1
                        
                        
2438 3d                 FIN_CONV:        Rts
                        
