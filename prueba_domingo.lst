2469 20 1f                              bra RETORNAR
                        
                        
                        
246b                    NotProc:
246b 18 0c 10 04 10 05                  Movb Tecla, Tecla_IN
2471 1c 10 00 02                        Bset Banderas, #2
2475 18 0b 0a 10 06                     Movb #10,Cont_Reb
247a 20 0e                              Bra RETORNAR
                        
247c                    DELETE:
247c 18 0b ff 10 04                     Movb #$FF,Tecla
2481 18 0b ff 10 05                     Movb #$FF,Tecla_IN
2486 1d 10 00 03                        Bclr Banderas, #3
                        
248a                    RETORNAR:
248a 3d                                 RTS
                        
                        
                        ;------------------------------------------------------------------------------
248b 18 0b ef 10 08     MUX_TECLADO:    movb #$EF,Patron                ; Patron inicial
2490 cc f0 00                           ldd #$F000                       ; final cuando se desplaza patron
                        
2493 18 0c 10 08 00 00  BUSCAR_COLUMNA: movb Patron,PORTA
2499 4f 00 08 18                        brclr PORTA,$08,columna2            ; Verificamos se la tecla est? en la columna2
249d 4f 00 04 15                        brclr PORTA,$04,columna1
24a1 4f 00 02 12                        brclr PORTA,$02,columna0
24a5 78 10 08                           lsl Patron                          ; Se desplaza el patron para verificar siguiente fila
24a8 cb 03                              addb #3                             ; Se suman 3 para aumentar esa cantidad en el array de posibilidades
24aa b1 10 08                           cmpa Patron
24ad 26 e4                              bne BUSCAR_COLUMNA
24af 18 0b ff 10 04                     movb #$FF,Tecla
24b4 3d                 TERMINAR:       rts
                        
24b5 52                 columna2:       incb                                ; Incrementa en 2 si salta ac?
24b6 52                 columna1:       incb                                ; Incrementa en 1 si salta ac?
24b7 ce 10 40           columna0:       ldx #Teclas
24ba 18 0d e5 10 04                     movb B,X,Tecla                      ; Se mueve la tecla encontrada
24bf 20 f3                              bra TERMINAR
                        
                        ;------------------------------------------------------------------------------
24c1 b6 10 05           FORMAR_ARRAY:   ldaa Tecla_IN                   ; valor ingresado
24c4 f6 10 07                           ldab Cont_TCL                   ; cantidad de numeros
24c7 ce 10 09                           ldx #Num_Array                   ; Posici?n del array
                        
24ca f1 10 03                           cmpb MAX_TCL                    ; comparamos si ya est? lleno
24cd 27 13                              beq ARRAY_LLENO
24cf c1 00                              cmpb #0                         ; vemos si est? vac?o
24d1 27 2a                              beq PRIMER_VAL
24d3 81 0b                              cmpa #$0B                       ; tecla borrar
24d5 27 42                              beq BORRAR
24d7 81 0e                              cmpa #$0E                       ; tecla enter
24d9 27 34                              beq ENTER
24db 6a e5                              staa b,x                        ; guarda en Num_array + cont_TCL
24dd 72 10 07                           inc Cont_TCL
24e0 20 3f                              bra end_formar
                        
