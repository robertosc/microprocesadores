233e 4d 32 02                           bclr PORTK,$02
2341 32                                 pula                    ;trae a
2342 84 0f                              anda #$0F               ;mascara parte baja
2344 48                                 lsla
2345 48                                 lsla
                        
2346 5a 32                              staa PORTK
2348 4d 32 01                           bclr PORTK,$01
234b 4c 32 02                           bset PORTK,$02
                        
234e 18 0c 10 2b 10 29                  movb D260uS,Cont_Delay  ; delay
