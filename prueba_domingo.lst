                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion PTH CALCULAR: Subrutina utilizada para la lectura de los sensores y subsecuentes
                        ;      calculos de velocidad, velocidad promedio, cantidad de vueltas y ticks necesarios para recorrer 100 m.
                        ;------------------------------------------------------------------------------
                        
                        
2151                    CALCULAR:
2151 1e 02 67 01 05                     brset PIFH,$01,PH0_ISR          ; se revisa cual interrupcion es
2156 1e 02 67 08 5f                     brset PIFH,$08,PH3_ISR
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion PTH0: Subrutina de atencion a la interrupcion por key wakeup en PH0. Simula el segundo sensor
                        ;      del velodromo y determina si la velocidad medida esta en el rango de velocidades valido, ademas de actualizar la velocidad promedio
                        ;      en el caso de que lo este.
                        ;      INPUTS: Cont_Reb, TICK_MED
                        ;      OUTPUTS: VELOC, VELPROM, VUELTAS, VELOCIDAD_VAL, CANT_VUELTAS_MAXIMA
                        ;      Formula para calcular la velocidad: VELOC = 9064/TICK_MED
                        ;      Formula para calcular/actualizar la velocidad promedio: VELPROM = (VELPROM*(VUELTAS-1) + VELOC)/VUELTAS
                        ;------------------------------------------------------------------------------
                        
215b                    PH0_ISR:
215b 1c 02 67 01                        bset PIFH,$01 ;se limpia la bandera
215f f7 10 06                           tst Cont_Reb ;se revisa si se terminaron los rebotes
2162 26 55                              bne FIN_PH0
                        
2164 18 0b 64 10 06                     movb #100,Cont_Reb ;se recarga cont_reb para control de rebotes
2169 fe 10 14                           ldx TICK_MED ;se lee la cantidad de ticks medidos
                        
216c 18 04 10 14 10 33                  movw TICK_MED,SAVE_MED
                        
2172 8e 00 81                           cpx #129 ;259 ticks para 35 km/h
2175 22 37                              bhi VEL_INVAL ;si es mayor a esto, la velocidad es menor a 35 km/h
2177 8e 00 30                           cpx #48 ;95 ticks para 95 km/h
217a 25 32                              blo VEL_INVAL ;si es menor a esto, la velocidad es mayor a 95 km/h
                        
                        
                        
                                        ;BSET BanderasEx,$01 ;se levanta la bandera de velocidad valida
217c 7e 10 31                           STX TICKS_TIME ;se guarda la cantidad de ticks necesarios para recorrer 55 m
217f cc 11 b4                           LDD #4532 ;D = 4532
2182 18 10                              IDIV ;X = (D/X) = 4532/TICKS = VELOC
2184 b7 54                              TFR X,D ;D = VELOC
2186 7b 10 11                           STAB VELOC ;la velocidad siempre es menor a 1 byte, por lo que se guarda en VELOC
                        
2189 72 10 12                           inc VUELTAS
                                        ;ldaa VELOC    ; se recarga veloc porque se necesita en a
218c f6 10 13                           ldab VELPROM ;velprom
218f b6 10 12                           ldaa VUELTAS   ;vuelt
2192 12                                 MUL
2193 b7 10                              tfr b,a
2195 f6 10 13                           ldab VELPROM
2198 18 16                              sba
219a f6 10 11                           ldab VELOC
219d 18 06                              aba
219f b7 01                              tfr a,b
21a1 87                                 clra
21a2 fe 10 12                           ldx VUELTAS
21a5 18 10                              idiv
21a7 b7 54                              tfr x,d
                        
21a9 7b 10 13           		stab VELPROM
                        
21ac 20 0b                              BRA FIN_PH0
                        
21ae                    VEL_INVAL:
21ae 18 0b ff 10 11                     movb #$FF,VELOC ;la velocidad medida es invalida, por lo que se desactiva la bandera de velocidad valida
21b3 1d 10 30 01                        bclr BanderasEx,$01
21b7 20 00                              bra FIN_PH0
                        
21b9                    FIN_PH0:
21b9 0b                                 rti
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion PTH3: Subrutina de atencion a la interrupcion por key wakeup en PH3. Simula el primer sensor
                        ;      del velodromo y se encarga de borrar TICK_MED, asi como indicar que se debe cargar el Mensaje Cargando en la pantalla LCD
                        ;      INPUTS: Cont_Reb
                        ;      OUTPUTS: TICK_MED, DISPLAY_CALC
                        ;------------------------------------------------------------------------------
21ba                    PH3_ISR:
21ba 1c 02 67 08                        bset PIFH,$08
21be f7 10 06                           tst Cont_Reb
21c1 26 12                              bne END_PH3
21c3 79 10 33                           clr SAVE_MED
21c6 1c 10 00 40                        BSET Banderas,$40 ;se levanta DISPLAY_CALC
21ca 18 0b 64 10 06                     MOVB #100,Cont_Reb
21cf 18 03 00 00 10 14                  MOVW #$0000,TICK_MED ;se borra TICK_MED
                        
21d5                    END_PH3:
21d5 0b                                 RTI
                        
                        
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion TCNT: Subrutina de atencion por timer overflow. Se encarga de incrementar TICK_MED y
                        ;      decrementar TICK_DIS y TICK_EN, esto con el objetivo de calcular la velocidad y marcar los tiempos en los que se deben actualizar
                        ;      las pantallas. Usa prescaler de 8, el tiempo de tick es dado por Ttick = 8*2^(16) / 24MHz, que es aproximadamente 21.8 ms
                        ;      OUTPUTS: TICK_MED, TICK_EN, TICK_DIS, PANT_FLG
                        ;------------------------------------------------------------------------------
                        
21d6                    TCNT_ISR:
21d6 4c 4f 80                           BSET TFLG2,$80 ;se limpia la bandera de interrupcion
21d9 fe 10 14                           LDX TICK_MED ;se carga TICK_MED y se compara si llego a $FFFF, para evitar overflow
21dc 8e ff ff                           CPX #$FFFF
21df 27 04                              BEQ CHECK_EN_TCNT
21e1 08                                 INX ;si no es $FFFF, se incrementa y se guarda el valor
21e2 7e 10 14                           STX TICK_MED
                        
21e5                    CHECK_EN_TCNT:
21e5 fe 10 0d                           LDX TICK_EN ;se carga TICK_EN y se ve si es 0. Si fuera 0, se debe levantar PANT_FLG
21e8 8e 00 00                           CPX #0
21eb 27 0b                              BEQ EN_IS_0_TCNT
21ed 8e ff ff                           CPX #$FFFF ;se ve si es $FFFF, lo cual significa que ya se llego a 0 y no se ha cargado un nuevo valor
21f0 27 10                              BEQ CHECK_DIS_TCNT
21f2 09                                 DEX ;si no se cumple ninguna las dos condiciones revisadas, se decrementa y guarda el valor
21f3 7e 10 0d                           STX TICK_EN
21f6 20 0a                              BRA CHECK_DIS_TCNT
                        
21f8                    EN_IS_0_TCNT:
21f8 18 03 ff ff 10 0d                  MOVW #$FFFF,TICK_EN
21fe 1c 10 00 08                        BSET Banderas,$08 ;se levanta PANT_FLG
                        
2202                    CHECK_DIS_TCNT:
2202 fe 10 0f                           LDX TICK_DIS ;se carga TICK_DIS y se ve si es 0. Si fuera 0, se debe desactivar PANT_FLG
2205 8e 00 00                           CPX #0
2208 27 0b                              BEQ DIS_IS_0_TCNT
220a 8e ff ff                           CPX #$FFFF ;se ve si es $FFFF, lo cual significa que ya se llego a 0 y no se ha cargado un nuevo valor
220d 27 10                              BEQ FIN_TCNT
220f 09                                 DEX ;si no se cumple ninguna las dos condiciones revisadas, se decrementa y guarda el valor
2210 7e 10 0f                           STX TICK_DIS
2213 20 0a                              BRA FIN_TCNT
                        
2215                    DIS_IS_0_TCNT:
2215 18 03 ff ff 10 0f                  MOVW #$FFFF,TICK_DIS
221b 1d 10 00 08                        BCLR Banderas,$08 ;se desactiva PANT_FLG
                        
221f                    FIN_TCNT:
221f 0b                                 RTI
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion ATD0: Subrutina utilizada para la conversion analogica digital del potenciometro
                        ;      de la tarjeta dragon 12, utilizado para controlar el brillo de los leds y las pantallas de 7 segmentos.
                        ;      Se toman 6 mediciones y se calcula el promedio.
                        ;      INPUTS: ADR00H,ADR01H,ADR02H,ADR03H,ADR04H,ADR05H
                        ;      OUTPUTS: BRILLO, DT
                        ;------------------------------------------------------------------------------
                        
2220                    ATD_ISR:
2220 ce 00 06                           ldx #6
2223 dc 90                              ldd ADR00H   ;Se calcula el promedio de las 6 medidas del potenciometro
2225 d3 92                              addd ADR01H
2227 d3 94                              addd ADR02H
2229 d3 96                              addd ADR03H
222b d3 98                              addd ADR04H
222d d3 9a                              addd ADR05H
222f 18 10                              idiv
2231 b7 54                              tfr X,D
2233 7b 10 0c                           stab POT ;Guardar el promedio
2236 86 14                              ldaa #20
2238 12                                 mul
2239 ce 00 ff                           ldx #255
223c 18 10                              idiv
223e b7 54                              tfr X,D
2240 7b 10 0b                           stab BRILLO
                        
2243 0b                                 RTI
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion RTI: Esta subrutina descuenta contadores
                        ;     siempre y cuando no sean cero. Los ticks del RTI duran 1.024 ms, por lo
                        ;     que si se cargan variables con X valor se pueden contar aproximadamente
                        ;     X milisegundos. Cont_Reb tiene un valor maximo de 10; se utiliza para
                        ;     suprimir rebotes contando ~10ms. Tambien lleva el tiempo para iniciar ciclos de conversion del ATD
                        ;     INPUTS: Cont_Reb, CONT_200
                        ;     OUTPUTS: Cont_Reb, CONT_200
                        ;------------------------------------------------------------------------------
2244 4c 37 80           RTI_ISR:        bset CRGFLG %10000000                 ;borra bandera de interrupcion RTI
2247 f7 10 06                           tst Cont_Reb
224a 27 03                              beq TIMER                       ;si llegaron los rebotes a 0, se termina la rubrutina
224c 73 10 06                           dec Cont_Reb
                        
                                                         ;Solo se decrementa si TIMER CUENTA no es cero
                        
224f f7 10 28           TIMER:          tst CONT_200
2252 26 0c                              bne NO_RESET
                                        ;Caso en que es cero y se reinicia
2254 18 0b c8 10 28                     Movb #200,CONT_200 ;Volvemos al contador con 200
2259 18 0b 87 00 85                     Movb #$87,ATD0CTL5
225e 20 03                              Bra FIN_RTI
                        
                        
2260 73 10 28           NO_RESET:           dec CONT_200                          ;Decrementamos el contador de rebotes si aun no ha llegado a cero
                        
                        
2263 0b                 FIN_RTI:        Rti
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion por output compare en el canal 4:
                        ;     Descuenta Cont_Delay, refresca cada 100 ms (5000ticks) los valores de
                        ;     DISP1-DISP4, multiplexa el bus del puerto B para mostrar informacion en
                        ;     los displays de 7 segmentos y los LEDS, y todo con un ciclo de trabajo
                        ;     variable que depende de DT.
                        ;     INPUTS: DT, Cont_Delay, LEDS, CONT_TICKS, DISP1-DISP4
                        ;     OUTPUTS: Cont_Delay, DISP1-DISP4
                        ;------------------------------------------------------------------------------
2264                    OC4_ISR:
2264 b6 10 29                           ldaa Cont_Delay                 ;Revisamos Cont_Delay para ver si hay que restarle
2267 81 00                              cmpa #0
2269 27 03                              beq CONTROL_PANTALLA            ; si ya es 0, pasamos a siguiente func
226b 73 10 29                           dec Cont_Delay                  ; decrementa
                        
226e                    CONTROL_PANTALLA:
226e fe 10 26                           ldx CONT_7SEG                   ; carga contador de 7seg
2271 09                                 dex                             ; le decrementa
2272 7e 10 26                           stx CONT_7SEG
2275 8e 00 00                           cpx #0
2278 26 0c                              bne CONTADOR_DISP               ; Si llega a 0, se le suman 5000 y volvemos a hacer conversiones
227a 18 03 13 88 10 26                  movw #5000,CONT_7SEG
2280 16 23 e4                           jsr CONV_BIN_BCD
2283 16 27 4e                           jsr BCD_7SEG
                        
2286                    CONTADOR_DISP:
2286 b6 10 24                           ldaa CONT_TICKS                 ; revisamos contador de ticks
2289 81 64                              cmpa #100
228b 26 11                              bne MUX
228d 87                                 clra                            ; si llega a 100, se borra
228e 7a 10 24                           staa CONT_TICKS
2291 72 10 23                           inc CONT_DIG                    ;cambia de digito para pasar a multiplexar
2294 f6 10 23                           ldab CONT_DIG
2297 c1 05                              cmpb #5                         ; Si llega a 5 se borra
2299 26 03                              bne MUX
229b 79 10 23                           clr CONT_DIG
                        
229e                    MUX:
229e f7 10 24                           tst CONT_TICKS                  ; si el contador no es 0 pasa a ver el ciclo de trabajo
22a1 26 5c                              bne DT_BRILLO
                        
22a3 18 0b 02 02 68                     movb #$02,PTJ                   ; cuando es 0 ponemos dato en un display
22a8 18 0b ff 02 58                     movb #$FF,PTP
                        
22ad f6 10 23                           ldab CONT_DIG                   ; cual display?
22b0 c1 00                              cmpb #0
22b2 27 19                              beq P4
22b4 c1 01                              cmpb #1
22b6 27 22                              beq P3
22b8 c1 02                              cmpb #2
22ba 27 2b                              beq P2
22bc c1 03                              cmpb #3
22be 27 34                              beq P1
                        
22c0 18 0b 00 02 68                     movb #$00,PTJ
22c5 18 0c 10 22 00 01                  movb LEDS,PORTB
22cb 20 32                              bra DT_BRILLO
22cd                    P4:
22cd 18 0b f7 02 58                     movb #$F7,PTP                         ;se habilita display 4
22d2 18 0c 10 21 00 01                  movb DISP4,PORTB
22d8 20 25                              bra DT_BRILLO
22da                    P3:
