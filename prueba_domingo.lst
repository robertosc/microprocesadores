2360 5a 32                              staa PORTK ;se carga parte alta del dato en el bus de datos.
2362 4c 32 03                           bset PORTK,$03 ;Se habilita el envio de dato y comunicacion con la LCD
2365 18 0c 10 2b 10 29                  movb D260us,Cont_Delay ;se inicia el retardo de 260us
236b 16 23 88                           jsr Delay
236e 4d 32 02                           bclr PORTK,$02 ;Se deshabilita comunicacion con la LCD
2371 32                                 pula ;se recupera el dato original de la pila
2372 84 0f                              anda #$0F ;Se deja solo el nibble inferior del dato
2374 48                                 lsla
2375 48                                 lsla ;se alinea nibble con bus datos en PORTK5-PORTK2.
2376 5a 32                              staa PORTK ;se carga parte baja del dato en el bus de datos.
2378 4c 32 03                           bset PORTK,$03 ;Se habilita envio de datos y comunicacion con la LCD
237b 18 0c 10 2b 10 29                  movb D260us,Cont_Delay ;se inicia el retardo de 260us.
2381 16 23 88                           jsr Delay
2384 4d 32 02                           bclr PORTK,$02 ;Se deshabilita comunicacion con la LCD
2387 3d                                 rts
                        
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina Delay: se mantiene en un loop cerrado hasta que Cont_Delay sea 0.
