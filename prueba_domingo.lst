2329 84 f0                              anda #$F0               ;mascara de parte alta
232b 44                                 lsra                    ;deja limpios los dos bits menos significativos
232c 44                                 lsra
                        
232d 5a 32                              staa PORTK              ;guarda a en portk
232f 4d 32 01                           bclr PORTK,$01          ;modif bits menos significativos
2332 4c 32 02                           bset PORTK,$02
                        
2335 18 0c 10 2b 10 29                  movb D260uS,Cont_Delay  ;delay
