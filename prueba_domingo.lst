22df 18 0c 10 20 00 01                  movb DISP3,PORTB
22e5 20 18                              bra DT_BRILLO
22e7                    P2:
22e7 18 0b fd 02 58                     movb #$FD,PTP                         ;se habilita display 2
22ec 18 0c 10 1f 00 01                  movb DISP2,PORTB
22f2 20 0b                              bra DT_BRILLO
22f4                    P1:
22f4 18 0b fe 02 58                     movb #$FE,PTP                         ;se habilita display 1
22f9 18 0c 10 1e 00 01                  movb DISP1,PORTB
22ff                    DT_BRILLO:
