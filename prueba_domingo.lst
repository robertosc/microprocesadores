2301 f6 10 0b                           ldab BRILLO
2304 12                                 mul
2305 7b 10 25                           stab DT
                        
2308 b6 10 24                           ldaa CONT_TICKS                 ; si el contador llega a ciclo, termina
230b b1 10 25                           cmpa DT
230e 26 0a                              bne FIN_OC4
2310 18 0b ff 02 58                     movb #$FF,PTP                         ;deshabilita displays de 7 segmentos
2315 18 0b 02 02 68                     movb #$02,PTJ                         ;deshabilita LEDS
231a                    FIN_OC4:
