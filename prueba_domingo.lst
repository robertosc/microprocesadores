24e4 26 0a                              bne ARRAY_LLENO_1
24e6 53                                 decb
24e7 18 08 e5 ff                        movb #$FF,b,x                    ; Para borrar reemplazamos valor actual con ff
24eb 73 10 07                           dec Cont_TCL
24ee 20 31                              bra end_formar
                        
