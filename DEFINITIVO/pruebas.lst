                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion PTH CALCULAR: Subrutina utilizada para la lectura de los sensores y subsecuentes
                        ;      calculos de velocidad, velocidad promedio, cantidad de vueltas y ticks necesarios para recorrer 100 m.
                        ;------------------------------------------------------------------------------
                        
                        
2197                    CALCULAR:
2197 1e 02 67 01 05                     brset PIFH,$01,PH0_ISR          ; se revisa cual interrupcion es
219c 1e 02 67 08 5a                     brset PIFH,$08,PH3_ISR
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion PTH0: Subrutina de atencion a la interrupcion por key wakeup en PH0. Simula el segundo sensor
                        ;      del velodromo y determina si la velocidad medida esta en el rango de velocidades valido, ademas de actualizar la velocidad promedio
                        ;      en el caso de que lo este.
                        ;      INPUTS: Cont_Reb, TICK_MED
                        ;      OUTPUTS: VELOC, VELPROM, VUELTAS, VELOCIDAD_VAL, CANT_VUELTAS_MAXIMA
                        ;      Formula para calcular la velocidad: VELOC = 9064/TICK_MED
                        ;      Formula para calcular/actualizar la velocidad promedio: VELPROM = (VELPROM*(VUELTAS-1) + VELOC)/VUELTAS
                        ;------------------------------------------------------------------------------
                                        
21a1                    PH0_ISR:
21a1 1c 02 67 01                        bset PIFH,$01 ;se limpia la bandera
21a5 f7 10 06                           tst Cont_Reb ;se revisa si se terminaron los rebotes
21a8 26 50                              bne FIN_PH0
                                        
21aa 18 0b 64 10 06                     movb #100,Cont_Reb ;se recarga cont_reb para control de rebotes
21af fe 10 14                           ldx TICK_MED ;se lee la cantidad de ticks medidos
                        
21b2 18 04 10 14 10 33                  movw TICK_MED,SAVE_MED
                        
21b8 8e 00 81                           cpx #129 ;259 ticks para 35 km/h
21bb 22 32                              bhi VEL_INVAL ;si es mayor a esto, la velocidad es menor a 35 km/h
21bd 8e 00 30                           cpx #48 ;95 ticks para 95 km/h
21c0 25 2d                              blo VEL_INVAL ;si es menor a esto, la velocidad es mayor a 95 km/h
                        
                        
                        
                                        ;BSET BanderasEx,$01 ;se levanta la bandera de velocidad valida
21c2 7e 10 31                           STX TICKS_TIME ;se guarda la cantidad de ticks necesarios para recorrer 55 m
21c5 cc 11 b4                           LDD #4532 ;D = 9064
21c8 18 10                              IDIV ;X = (D/X) = 9064/TICKS = VELOC
21ca b7 54                              TFR X,D ;D = VELOC
21cc 7b 10 11                           STAB VELOC ;la velocidad siempre es menor a 1 byte, por lo que se guarda en VELOC
21cf b6 10 13                           LDAA VELPROM ;se carga la velocidad promedio en A
21d2 f6 10 12                           LDAB VUELTAS ;se carga la cantidad de vueltas-1 en B
21d5 12                                 MUL ;D = VELPROM*(VUELTAS-1)
21d6 b7 45                              TFR D,X ;X = VELPROM*(VUELTAS-1)
21d8 f6 10 11                           LDAB VELOC ;B = VELOC
21db 1a e5                              ABX ;X = VELPROM*(VUELTAS-1) + VELOC
21dd 72 10 12                           INC VUELTAS ;se actualiza el valor de VUELTAS
21e0 f6 10 12                           LDAB VUELTAS
21e3 87                                 CLRA ;D = VUELTAS
21e4 b7 c5                              XGDX ;D = VELPROM*(VUELTAS-1) + VELOC, X = VUELTAS
21e6 18 10                              IDIV ;X = (D/X) = VELPROM
21e8 b7 54                              TFR X,D ;D = VELPROM
21ea 7b 10 13                           STAB VELPROM ;se actualiza la velocidad promedio
                                        ;LDAB VUELTAS
                                        ;CMPB NumVueltas ;se ve si se llego a NumVueltas
                                        ;BLO MAX_VUELTAS_ALCANZADAS ;si no ha llegado a NumVueltas, retorna
                                        ;BSET BanderasEx,$02 ;si se llego a NumVueltas se levanta CANT_VUELTAS_MAXIMA
21ed 20 0b                              BRA FIN_PH0
                        
21ef                    VEL_INVAL:
21ef 18 0b ff 10 11                     movb #$FF,VELOC ;la velocidad medida es invalida, por lo que se desactiva la bandera de velocidad valida
21f4 1d 10 30 01                        bclr BanderasEx,$01
21f8 20 00                              bra FIN_PH0
                        
21fa                    MAX_VUELTAS_ALCANZADAS:
                                        ;bclr BanderasEx,$02
                        
21fa                    FIN_PH0:
21fa 0b                                 rti
                                        
