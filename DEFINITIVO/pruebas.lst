                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion TCNT: Subrutina de atencion por timer overflow. Se encarga de incrementar TICK_MED y
                        ;      decrementar TICK_DIS y TICK_EN, esto con el objetivo de calcular la velocidad y marcar los tiempos en los que se deben actualizar
                        ;      las pantallas. Usa prescaler de 8, el tiempo de tick es dado por Ttick = 8*2^(16) / 24MHz, que es aproximadamente 21.8 ms
                        ;      OUTPUTS: TICK_MED, TICK_EN, TICK_DIS, PANT_FLG
                        ;------------------------------------------------------------------------------
                        
2217                    TCNT_ISR:
2217 4c 4f 80                           BSET TFLG2,$80 ;se limpia la bandera de interrupcion
221a fe 10 14                           LDX TICK_MED ;se carga TICK_MED y se compara si llego a $FFFF, para evitar overflow
221d 8e ff ff                           CPX #$FFFF
2220 27 04                              BEQ CHECK_EN_TCNT
2222 08                                 INX ;si no es $FFFF, se incrementa y se guarda el valor
2223 7e 10 14                           STX TICK_MED
                        
2226                    CHECK_EN_TCNT:
2226 fe 10 0d                           LDX TICK_EN ;se carga TICK_EN y se ve si es 0. Si fuera 0, se debe levantar PANT_FLG
2229 8e 00 00                           CPX #0
222c 27 0b                              BEQ EN_IS_0_TCNT
222e 8e ff ff                           CPX #$FFFF ;se ve si es $FFFF, lo cual significa que ya se llego a 0 y no se ha cargado un nuevo valor
2231 27 10                              BEQ CHECK_DIS_TCNT
2233 09                                 DEX ;si no se cumple ninguna las dos condiciones revisadas, se decrementa y guarda el valor
2234 7e 10 0d                           STX TICK_EN
2237 20 0a                              BRA CHECK_DIS_TCNT
                                        
2239                    EN_IS_0_TCNT:
2239 18 03 ff ff 10 0d                  MOVW #$FFFF,TICK_EN
223f 1c 10 00 08                        BSET Banderas,$08 ;se levanta PANT_FLG
                        
2243                    CHECK_DIS_TCNT:
2243 fe 10 0f                           LDX TICK_DIS ;se carga TICK_DIS y se ve si es 0. Si fuera 0, se debe desactivar PANT_FLG
2246 8e 00 00                           CPX #0
2249 27 0b                              BEQ DIS_IS_0_TCNT
224b 8e ff ff                           CPX #$FFFF ;se ve si es $FFFF, lo cual significa que ya se llego a 0 y no se ha cargado un nuevo valor
224e 27 10                              BEQ FIN_TCNT
2250 09                                 DEX ;si no se cumple ninguna las dos condiciones revisadas, se decrementa y guarda el valor
2251 7e 10 0f                           STX TICK_DIS
2254 20 0a                              BRA FIN_TCNT
                        
2256                    DIS_IS_0_TCNT:
2256 18 03 ff ff 10 0f                  MOVW #$FFFF,TICK_DIS
225c 1d 10 00 08                        BCLR Banderas,$08 ;se desactiva PANT_FLG
                                        
2260                    FIN_TCNT:
2260 0b                                 RTI
                            
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion ATD0: Subrutina utilizada para la conversion analogica digital del potenciometro
                        ;      de la tarjeta dragon 12, utilizado para controlar el brillo de los leds y las pantallas de 7 segmentos.
                        ;      Se toman 6 mediciones y se calcula el promedio.
                        ;      INPUTS: ADR00H,ADR01H,ADR02H,ADR03H,ADR04H,ADR05H
                        ;      OUTPUTS: BRILLO, DT
                        ;------------------------------------------------------------------------------
                            
2261                    ATD_ISR:
2261 ce 00 06                           ldx #6
2264 dc 90                              ldd ADR00H   ;Se calcula el promedio de las 6 medidas del potenciometro
2266 d3 92                              addd ADR01H
2268 d3 94                              addd ADR02H
226a d3 96                              addd ADR03H
226c d3 98                              addd ADR04H
226e d3 9a                              addd ADR05H
2270 18 10                              idiv
2272 b7 54                              tfr X,D
2274 7b 10 0c                           stab POT ;Guardar el promedio
2277 86 14                              ldaa #20
2279 12                                 mul
227a ce 00 ff                           ldx #255
227d 18 10                              idiv
227f b7 54              		tfr X,D
2281 7b 10 0b                           stab BRILLO
                        
2284 0b                                 RTI
                            
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion RTI: Esta subrutina descuenta contadores
                        ;     siempre y cuando no sean cero. Los ticks del RTI duran 1.024 ms, por lo
                        ;     que si se cargan variables con X valor se pueden contar aproximadamente
                        ;     X milisegundos. Cont_Reb tiene un valor maximo de 10; se utiliza para
                        ;     suprimir rebotes contando ~10ms. Tambien lleva el tiempo para iniciar ciclos de conversion del ATD
                        ;     INPUTS: Cont_Reb, CONT_200
                        ;     OUTPUTS: Cont_Reb, CONT_200
                        ;------------------------------------------------------------------------------
2285 4c 37 80           RTI_ISR:        bset CRGFLG %10000000                 ;borra bandera de interrupcion RTI
2288 f7 10 06                           tst Cont_Reb
228b 27 03                              beq TIMER                       ;si llegaron los rebotes a 0, se termina la rubrutina
228d 73 10 06                           dec Cont_Reb
                        
                                                         ;Solo se decrementa si TIMER CUENTA no es cero
                        
2290 f7 10 28           TIMER:          tst CONT_200
2293 26 0c                              bne NO_RESET
                                        ;Caso en que es cero y se reinicia
2295 18 0b c8 10 28                     Movb #200,CONT_200 ;Volvemos al contador con 200
229a 18 0b 87 00 85                     Movb #$87,ATD0CTL5
229f 20 03                              Bra FIN_RTI
                        
                        
22a1 73 10 28           NO_RESET:           dec CONT_200                          ;Decrementamos el contador de rebotes si aun no ha llegado a cero
                        
                        
22a4 0b                 FIN_RTI:        Rti
                                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina de servicio a interrupcion por output compare en el canal 4:
                        ;     Descuenta Cont_Delay, refresca cada 100 ms (5000ticks) los valores de
                        ;     DISP1-DISP4, multiplexa el bus del puerto B para mostrar informacion en
                        ;     los displays de 7 segmentos y los LEDS, y todo con un ciclo de trabajo
                        ;     variable que depende de DT.
                        ;     INPUTS: DT, Cont_Delay, LEDS, CONT_TICKS, DISP1-DISP4
                        ;     OUTPUTS: Cont_Delay, DISP1-DISP4
                        ;------------------------------------------------------------------------------
22a5                    OC4_ISR:
22a5 b6 10 29                           ldaa Cont_Delay                 ;Revisamos Cont_Delay para ver si hay que restarle
22a8 81 00                              cmpa #0
22aa 27 03                              beq CONTROL_PANTALLA            ; si ya es 0, pasamos a siguiente func
22ac 73 10 29                           dec Cont_Delay                  ; decrementa
                        
22af                    CONTROL_PANTALLA:
22af fe 10 26                           ldx CONT_7SEG                   ; carga contador de 7seg
22b2 09                                 dex                             ; le decrementa
22b3 7e 10 26                           stx CONT_7SEG
22b6 8e 00 00                           cpx #0
22b9 26 0c                              bne CONTADOR_DISP               ; Si llega a 0, se le suman 5000 y volvemos a hacer conversiones
22bb 18 03 13 88 10 26                  movw #5000,CONT_7SEG
22c1 16 24 25                           jsr CONV_BIN_BCD
22c4 16 27 4c                           jsr BCD_7SEG
                        
22c7                    CONTADOR_DISP:
22c7 b6 10 24                           ldaa CONT_TICKS                 ; revisamos contador de ticks
22ca 81 64                              cmpa #100
22cc 26 11                              bne MUX
22ce 87                                 clra                            ; si llega a 100, se borra
22cf 7a 10 24                           staa CONT_TICKS
22d2 72 10 23                           inc CONT_DIG                    ;cambia de digito para pasar a multiplexar
22d5 f6 10 23                           ldab CONT_DIG
22d8 c1 05                              cmpb #5                         ; Si llega a 5 se borra
22da 26 03                              bne MUX
22dc 79 10 23                           clr CONT_DIG
                        
22df                    MUX:
22df f7 10 24                           tst CONT_TICKS                  ; si el contador no es 0 pasa a ver el ciclo de trabajo
22e2 26 5c                              bne DT_BRILLO
                        
22e4 18 0b 02 02 68                     movb #$02,PTJ                   ; cuando es 0 ponemos dato en un display
22e9 18 0b ff 02 58                     movb #$FF,PTP
                        
22ee f6 10 23                           ldab CONT_DIG                   ; cual display?
22f1 c1 00                              cmpb #0
22f3 27 19                              beq P4
22f5 c1 01                              cmpb #1
22f7 27 22                              beq P3
22f9 c1 02                              cmpb #2
22fb 27 2b                              beq P2
22fd c1 03                              cmpb #3
22ff 27 34                              beq P1
                        
2301 18 0b 00 02 68                     movb #$00,PTJ
2306 18 0c 10 22 00 01                  movb LEDS,PORTB
230c 20 32                              bra DT_BRILLO
230e                    P4:
230e 18 0b f7 02 58                     movb #$F7,PTP                         ;se habilita display 4
2313 18 0c 10 21 00 01                  movb DISP4,PORTB
2319 20 25                              bra DT_BRILLO
231b                    P3:
231b 18 0b fb 02 58                     movb #$FB,PTP                         ;se habilita display 3
2320 18 0c 10 20 00 01                  movb DISP3,PORTB
2326 20 18                              bra DT_BRILLO
2328                    P2:
