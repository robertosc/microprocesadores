                        ;------------------------------------------------------------------------------
                        ;   Subrutina BIN_BCD: esta subrutina realiza la conversion de un numero
                        ;     binario entre 0 y 99 (inclusivos) a su representacion en BCD. El numero
                        ;     a convertir se recibe como parametro por el registro A. El resultado en
                        ;     BCD se devuelve por la variable BCD_L, donde el nibble mas significativo son
                        ;     las decenas y el menos significativo las unidades.
                        ;------------------------------------------------------------------------------
23f3                    BIN_BCD:
                        
23f3 c6 07                              Ldab #7  ; Contador B=15
23f5 79 10 1a                           Clr BCD_L
                        
23f8                    lazo:
23f8 48                                 Lsla
23f9 75 10 1a                           Rol BCD_L  ;Lo mismo para la variable BCD_L y BCD_H
23fc 36                                 Psha
                        
23fd b6 10 1a                           Ldaa BCD_L ;Cargamos en A el BCD_L
2400 84 0f                              Anda #$0F  ;Tomamos solo en cuenta los 4LSB
2402 81 05                              Cmpa #5   ;Comparamos con 5
2404 25 02                              Blo men031  ;Si es menor, salte a men031
2406 8b 03                              Adda #3  ;En caso de mayor, sume 3
                        
2408                    men031
2408 7a 10 1d                           Staa LOW  ;Guardamos temporalmente el resultado anterior
                        
240b b6 10 1a                           Ldaa BCD_L
240e 84 f0                              Anda #$F0 ;En A tenemos cargado del bit 4 al 7
2410 81 50                              Cmpa #$50  ;Comparamos con $50
2412 25 02                              Blo men301
2414 8b 30                              Adda #$30   ;Si es mayor, sume 30
                        
2416                    men301
2416 bb 10 1d                           Adda LOW   ;Se suman los bits para obtener los 4 LSB de resultado
2419 7a 10 1a                           Staa BCD_L  ;Se guarda el resultado
241c 32                                 Pula
241d 04 31 d8                           Dbne b, lazo
                        
2420 48                                 Lsla
2421 75 10 1a                           Rol BCD_L
                        
2424 3d                                 Rts
                        
                        ;------------------------------------------------------------------------------
                        ;   Subrutina CONV_BIN_BCD: recibe como parametros de entrada las variables BIN1 y
                        ;     BIN2 y realiza la conversion a BCD de cada una de estas variables. Luego de la conversion, si
                        ;     el numero es menor que 10 significa que el display de 7 segmentos utilizado
                        ;     para las decenas no es necesario que este encendido; en este caso se escribe
                        ;     $B en el nibble mas significativo de BCD1 y BCD2 para indicarlo. Carga $BB
                        ;     en BCD1 o BCD2 dependiendo de si deben estar apagados, mientras que carga
                        ;     $AA si deben tener una raya.
                        ;     INPUTS: BIN1, BIN2
                        ;     OUTPUTS: BCD1, BCD2
                        ;------------------------------------------------------------------------------
2425                    CONV_BIN_BCD:
2425 b6 10 16                           Ldaa BIN1
                        
2428 81 bb                              Cmpa #$BB
242a 27 06                              Beq BIN1_BB
                        
242c 81 aa                              Cmpa #$AA
242e 27 09              		Beq BIN1_AA
                        
2430 20 0e              		Bra BIN1_CALC
                        
2432 18 0b bb 10 18     BIN1_BB:	Movb #$BB,BCD1
2437 20 16              		Bra BIN2_CHECK
                        
2439 18 0b aa 10 18     BIN1_AA:	Movb #$AA,BCD1
243e 20 0f              		Bra BIN2_CHECK
                        
                        
2440                    BIN1_CALC:
2440 16 23 f3                           Jsr BIN_BCD ;Pasamos BIN1 a BCD
2443 b6 10 1a                           Ldaa BCD_L
2446 81 0a                              Cmpa #10
2448 24 02                              Bhs mayor1
244a 8b b0                              Adda #$B0 ;Si solo tiene un digito, agrega B como "decenas"
                        
244c 7a 10 18           mayor1          Staa BCD1 ;Guardamos el valor en BCD1
                        
                        
244f b6 10 17           BIN2_CHECK:	Ldaa BIN2
                        
2452 81 bb              		Cmpa #$BB
2454 27 06              		Beq BIN2_BB
                        
2456 81 aa              		Cmpa #$AA
2458 27 09              		Beq BIN2_AA
                        
245a 20 0e              		Bra BIN2_CALC
                        
245c 18 0b bb 10 19     BIN2_BB:	Movb #$BB,BCD2
2461 20 16              		Bra FIN_CONV
                        
2463 18 0b aa 10 19     BIN2_AA:	Movb #$AA,BCD2
2468 20 0f              		Bra FIN_CONV
                        
                        
246a                    BIN2_CALC:
246a 16 23 f3                           Jsr BIN_BCD ;Pasamos BIN1 a BCD
246d b6 10 1a                           Ldaa BCD_L
2470 81 0a                              Cmpa #10
2472 24 02                              Bhs mayor2
2474 8b b0                              Adda #$B0 ;Si solo tiene un digito, agrega B como "decenas"
                        
2476 7a 10 19           mayor2          Staa BCD2 ;Guardamos el valor en BCD1
                        
                        
2479 3d                 FIN_CONV:	Rts
                        
